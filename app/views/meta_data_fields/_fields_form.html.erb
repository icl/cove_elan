<% if object.meta_data_group %>
  <% object.meta_data_group.meta_data_fields.each do |mdf| %>
    <%= f.simple_fields_for :meta_data_fields do |m| %>
      <%= m.input mdf.name, :as => mdf.meta_data_field_type.field_type, 
        :input_html => { :value => (MetaDataHelper.get_field_values mdf, object) } %>
    <% end %>
  <% end %>
<% end %>
